<?xml version="1.0"?>
<pullcase>
<description>Yahoo Trending</description>
<site>http://www.yahoo.com</site>
<firefoxprofile>
	<preferences>browser.download.manager.showWhenStarting</preferences>
	<values>false</values>
</firefoxprofile>
<steps>
<step1>
<command>type</command>
<parameter>.input-query</parameter>
<value>test</value>
</step1>
<step2>
<command>click</command>
<parameter>.searchsubmit</parameter>
</step2>
</steps>
<root>results</root>
<element_head>result</element_head>
<elements>
<element1>
<input>.Sr > .compTitle > .title > a</input>
<output>title</output>
</element1>
<element2>
<input>.Sr > .compText > p</input>
<output>description</output>
</element2>
</elements>
</pullcase>
